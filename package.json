{
  "name": "ecommerce-bmaudio",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "clean": "turbo clean",
    "db:generate": "pnpm --filter @bmaudio/api prisma generate",
    "db:migrate": "pnpm --filter @bmaudio/api prisma migrate dev",
    "db:push": "pnpm --filter @bmaudio/api prisma db push",
    "db:studio": "pnpm --filter @bmaudio/api prisma studio",
    "db:seed": "pnpm --filter @bmaudio/api prisma db seed"
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=20.0.0"
  }
}
